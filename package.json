{
  "name": "@robonaut/website",
  "version": "0.0.5",
  "description": "Robonaut website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.ts --progress --profile --color",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "./scripts/fix_typings.sh",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "version:bump": "npm run version:patch && npm run version:commit",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:commit": "git add package.json package-lock.json && git commit -n -m \"v${npm_package_version}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robonaut/robonaut.be.git"
  },
  "keywords": [
    "WWS",
    "portal"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://robonaut.be",
  "devDependencies": {
    "@types/copy-webpack-plugin": "6.4.0",
    "@types/marked": "1.2.1",
    "@types/node": "14.14.19",
    "@types/prismjs": "1.16.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.15",
    "@types/react-router-dom": "5.1.7",
    "@types/react-table": "7.0.26",
    "@types/styled-components": "5.1.7",
    "@types/webpack-dev-server": "3.11.1",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "copy-webpack-plugin": "7.0.0",
    "date-fns": "2.16.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.6",
    "i18next": "19.8.4",
    "marked": "1.2.7",
    "prettier": "2.2.1",
    "prismjs": "1.23.0",
    "raw-loader": "4.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.5",
    "react-icons": "4.1.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-table": "7.6.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "styled-components": "5.2.1",
    "ts-loader": "8.0.13",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run lint"
    }
  }
}

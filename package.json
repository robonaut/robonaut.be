{
  "name": "@robonaut/website",
  "version": "0.0.18",
  "description": "Robonaut website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && WEBPACK_ENV=production webpack --config webpack.config.ts --progress --profile --color",
    "coverage": "",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "git:hooks": "git config core.hooksPath .githooks",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "./scripts/fix_typings.sh",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "version:bump": "npm run version:patch && npm run version:commit",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:commit": "git add package.json package-lock.json && git commit -n -m \"v${npm_package_version}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robonaut/robonaut.be.git"
  },
  "keywords": [
    "WWS",
    "portal"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://robonaut.be",
  "devDependencies": {
    "@types/copy-webpack-plugin": "6.4.1",
    "@types/marked": "2.0.1",
    "@types/node": "14.14.37",
    "@types/prismjs": "1.16.4",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-table": "7.0.29",
    "@types/styled-components": "5.1.9",
    "@types/webpack-dev-server": "3.11.3",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "copy-webpack-plugin": "8.1.1",
    "date-fns": "2.19.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "5.3.1",
    "i18next": "20.2.1",
    "marked": "2.0.1",
    "prettier": "2.2.1",
    "prismjs": "1.24.0",
    "raw-loader": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.8.12",
    "react-icons": "4.2.0",
    "react-redux": "7.2.3",
    "react-router-dom": "5.2.0",
    "react-table": "7.6.3",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.9",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "styled-components": "5.2.3",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.31.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {}
}

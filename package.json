{
  "name": "@robonaut/website",
  "version": "0.0.18",
  "description": "Robonaut website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && WEBPACK_ENV=production webpack --config webpack.config.ts --progress --profile --color",
    "coverage": "",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "git:hooks": "git config core.hooksPath .githooks",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "./scripts/fix_typings.sh",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "version:bump": "npm run version:patch && npm run version:commit",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:commit": "git add package.json package-lock.json && git commit -n -m \"v${npm_package_version}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robonaut/robonaut.be.git"
  },
  "keywords": [
    "WWS",
    "portal"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://robonaut.be",
  "devDependencies": {
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/marked": "4.0.3",
    "@types/node": "16.11.7",
    "@types/prismjs": "1.26.0",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-table": "7.7.12",
    "@types/styled-components": "5.1.25",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "copy-webpack-plugin": "11.0.0",
    "date-fns": "2.28.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-webpack-plugin": "5.5.0",
    "i18next": "21.8.9",
    "marked": "4.0.17",
    "prettier": "2.7.0",
    "prismjs": "1.28.0",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "11.17.2",
    "react-icons": "4.4.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-table": "7.8.0",
    "redux": "4.2.0",
    "redux-devtools-extension": "2.13.9",
    "redux-saga": "1.1.3",
    "reselect": "4.1.6",
    "styled-components": "5.3.5",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.3",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  }
}

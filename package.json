{
  "name": "robonaut.be",
  "version": "0.0.5",
  "description": "My personal website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "IS_WEBPACK=true webpack-dev-server --config webpack.config.ts",
    "build": "rm -rf dist && IS_WEBPACK=true NODE_ENV=production webpack --mode production --config webpack.config.ts --progress --profile --colors",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "version:patch": "HUSKY_SKIP_HOOKS=1 npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robonaut/robonaut.github.io.git"
  },
  "keywords": [
    "website"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://github.com/robonaut/robonaut.be#readme",
  "devDependencies": {
    "@types/html-webpack-plugin": "3.2.2",
    "@types/marked": "0.7.4",
    "@types/node": "13.13.4",
    "@types/prismjs": "1.16.0",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/styled-components": "5.1.0",
    "@types/webpack": "4.41.12",
    "@types/webpack-bundle-analyzer": "2.13.3",
    "@types/webpack-dev-server": "3.10.1",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "raw-loader": "4.0.1",
    "ts-loader": "7.0.2",
    "ts-node": "8.9.1",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@reach/router": "1.3.3",
    "marked": "1.0.0",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run lint:fix"
    }
  }
}

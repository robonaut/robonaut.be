{
  "name": "@robonaut/website",
  "version": "0.0.18",
  "description": "Robonaut website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && WEBPACK_ENV=production webpack --config webpack.config.ts --progress --profile --color",
    "coverage": "",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "git:hooks": "git config core.hooksPath .githooks",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "./scripts/fix_typings.sh",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "version:bump": "npm run version:patch && npm run version:commit",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:commit": "git add package.json package-lock.json && git commit -n -m \"v${npm_package_version}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robonaut/robonaut.be.git"
  },
  "keywords": [
    "WWS",
    "portal"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://robonaut.be",
  "devDependencies": {
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/marked": "5.0.2",
    "@types/node": "22.7.7",
    "@types/prismjs": "1.26.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@types/react-table": "7.7.20",
    "@types/styled-components": "5.1.34",
    "@types/webpack-env": "1.18.5",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "copy-webpack-plugin": "12.0.2",
    "date-fns": "4.1.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "html-webpack-plugin": "5.6.2",
    "i18next": "23.16.2",
    "marked": "14.1.3",
    "prettier": "3.3.3",
    "prismjs": "1.29.0",
    "raw-loader": "4.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.0.3",
    "react-icons": "5.3.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.27.0",
    "react-table": "7.8.0",
    "redux": "4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "1.3.0",
    "reselect": "5.1.1",
    "styled-components": "6.1.13",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "webpack": "5.95.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  }
}

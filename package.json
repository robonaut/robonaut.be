{
  "name": "robonaut.be",
  "version": "0.0.1",
  "description": "My personal website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node server --watch",
    "build": "rm -rf dist && NODE_ENV=production webpack --mode production --config webpack/webpack.config.ts --progress --profile --colors",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "postinstall": "rm -rf node_modules/@types/react-native"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robonaut/robonaut.github.io.git"
  },
  "keywords": [
    "website"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://github.com/robonaut/robonaut.be#readme",
  "devDependencies": {
    "@types/html-webpack-plugin": "3.2.2",
    "@types/node": "13.9.2",
    "@types/reach__router": "1.3.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/styled-components": "5.0.1",
    "@types/webpack": "4.41.7",
    "@types/webpack-dev-server": "3.10.1",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-simple-import-sort": "5.0.2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "4.2.3",
    "prettier": "1.19.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.7.0",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@reach/router": "1.3.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run lint:fix"
    }
  }

{
  "name": "@robonaut/website",
  "version": "0.0.1",
  "description": "Robonaut website",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && NODE_ENV=production webpack --mode production --config webpack.config.ts --progress --profile --color",
    "lint": "eslint --ext .tsx,.ts ./src/",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "./scripts/fix_typings.sh",
    "test": "",
    "typecheck": "tsc -p tsconfig.json",
    "version:patch": "HUSKY_SKIP_HOOKS=1 npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robonaut/robonaut.be.git"
  },
  "keywords": [
    "WWS",
    "portal"
  ],
  "author": "ilja@robonaut.be",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robonaut/robonaut.be/issues"
  },
  "homepage": "https://robonaut.be",
  "devDependencies": {
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.4",
    "@types/webpack-dev-server": "3.11.1",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-simple-import-sort": "6.0.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "ts-loader": "8.0.11",
    "ts-node": "9.1.0",
    "typescript": "4.1.2",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run lint:fix"
    }
  }
}

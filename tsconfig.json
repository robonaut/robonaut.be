{
    "compilerOptions": {
      "allowJs": false,
      "outDir": "./dist/",
      "sourceMap": true,
      "declaration": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "lib": [ "es2015", "dom" ],
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
    }
}

{
    "compilerOptions": {
      "allowJs": false,
      "outDir": "./dist/",
      "sourceMap": true,
      "declaration": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "resolveJsonModule": true,
    }
}
